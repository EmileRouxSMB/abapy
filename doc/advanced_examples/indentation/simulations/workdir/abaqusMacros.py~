    session.viewports['Viewport: 1'].odbDisplay.display.setValues(plotState=(
        CONTOURS_ON_DEF, ))
    odb = session.odbs['/home/lcharleux/Documents/Prog/modules/python/testing/vonmises/workdir/VonMises_1.odb']
    session.writeFieldReport(fileName='abaqus2.rpt', append=OFF, 
        sortItem='Node Label', odb=odb, step=2, frame=1, 
